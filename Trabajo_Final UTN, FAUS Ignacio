   ],
   "source": [
    "plt.scatter(df.index,df[\"gdp\"], label=\"datos\", alpha=0.8)\n",
    "plt.scatter(outliers.index,outliers[\"gdp\"], color=\"red\", label=\"outliers\")\n",
    "plt.title(\"Grafico de Outliers\")\n",
    "plt.legend()\n",
    "plt.show()"
   ]
