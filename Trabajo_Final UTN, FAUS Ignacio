   "source": [
    "sns.boxplot(data=df, y=\"gdp\",flierprops={'markerfacecolor': 'red', 'marker': 'o'})\n",
    "plt.title(\"Diagrama de Caja de la Variable PBI\")\n",
    "plt.ylabel(\"PBI\")\n",
    "plt.grid()\n",
    "plt.show()"
   ]
