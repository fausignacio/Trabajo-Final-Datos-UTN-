   "cell_type": "code",
   "execution_count": 28,
   "id": "e7366d9b-9678-4fa4-924c-c69864425be3",
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "Un valor de % de analfabetismo por debajo de: 0 es un outlier\n",
      "Un valor de % de analfabetismo por arriba de: 6.230760000000001 es un outlier\n"
     ]
    }
   ],
   "source": [
    "Q1= df[\"illiteracy\"].quantile(0.25)\n",
    "Q3= df[\"illiteracy\"].quantile(0.75)\n",
    "iqr= Q3-Q1\n",
    "umbral_superior = Q3 + 1.50*iqr\n",
    "umbral_inferior = max(0,Q1 - 1.5 * iqr)\n",
    "print(\"Un valor de % de analfabetismo por debajo de:\",umbral_inferior,\"es un outlier\")\n",
    "print(\"Un valor de % de analfabetismo por arriba de:\",umbral_superior,\"es un outlier\")"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 29,
